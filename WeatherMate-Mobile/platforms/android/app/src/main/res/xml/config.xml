<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Geolocation">
        <param name="android-package" value="org.apache.cordova.geolocation.Geolocation" />
    </feature>
    <name>WeatherMate</name>
    <description>Your Ultimate Weather and Forecast Viewer</description>
    <author email="hoangson091104@gmail.com" href="https://hoangsonww.github.io/My-Portfolio-Website/">
        David Nguyen
    </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <icon density="xxxhdpi" src="www/icons/AndroidIcon.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
